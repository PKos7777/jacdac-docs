{
  "@type": "Interface",
  "@id": "dtmi:jacdac:services:x1e5758b5;1",
  "displayName": "HID Adapter",
  "description": {
    "en": "A service for configuring how Jacdac device map to HID input events. Users can have multiple configurations and swap between them by writing to `current_configuration`."
  },
  "contents": [
    {
      "@type": "Property",
      "name": "num_configurations",
      "@id": "dtmi:jacdac:x1e5758b5:rw:numconfigurations;1",
      "description": {
        "en": "The number of configurations stored on the server."
      },
      "schema": "integer",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "current_configuration",
      "@id": "dtmi:jacdac:x1e5758b5:rw:currentconfiguration;1",
      "description": {
        "en": "The current configuration the server is using."
      },
      "schema": "integer",
      "writable": true
    },
    {
      "@type": [
        "Telemetry",
        "Event"
      ],
      "name": "changed",
      "@id": "dtmi:jacdac:x1e5758b5:event:changed;1",
      "description": {
        "en": "Event that notifies clients that the server has swapped to a new configuration or changed key bindings."
      },
      "schema": "integer"
    }
  ],
  "@context": [
    "dtmi:iotcentral:context;2",
    "dtmi:dtdl:context;2"
  ]
}